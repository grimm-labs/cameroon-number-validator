{
  "name": "cameroon-number-validator",
  "version": "1.0.0",
  "description": "Lightweight library to validate Cameroonian phone numbers, identify carriers (MTN, Orange, Camtel, Nexttel, Yoomee), and format to E.164.",
  "author": "",
  "license": "MIT",
  "keywords": ["cameroon", "phone", "validator", "e164"],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "tsup": "^8.0.0",
    "typescript": "^5.1.3"
  }
}